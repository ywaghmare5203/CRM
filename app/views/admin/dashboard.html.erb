</audio>
       <embed height="100" width="100" src="public/system/musics/1/original/32_EK_TERA_SAATH.mp3" />

      <%# @songs.each do |song| %>
        <%#*<li class="song">%>
          <%#= link_to karaoke_path(song.id) do %>
