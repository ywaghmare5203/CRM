          <%# end %>
          <%#*<p class="song_title"><%= song.title %></p>%>
          <%#*<p class="song_artist">by <%= song.artist.name %></p>%>
        <%#*</li>%>
      <%# end %>
    </ul>
  </div>
</div>





























































